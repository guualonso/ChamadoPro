<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox spacing="10" alignment="CENTER" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml"
      fx:controller="com.chamadopro.controller.LoginController">
    <Label text="ChamadoPro - Login"/>
    <TextField fx:id="emailField" promptText="E-mail"/>
    <PasswordField fx:id="senhaField" promptText="Senha"/>
    <Button text="Entrar" onAction="#handleLoginButtonAction"/>

</VBox>
